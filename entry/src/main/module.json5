{
  module: {
    name: "entry",
    type: "entry",
    description: "$string:module_desc",
    mainElement: "EntryAbility",
    deviceTypes: ["default", "tablet"],
    deliveryWithInstall: true,
    installationFree: false,
    pages: "$profile:main_pages",
    requestPermissions: [
      {
        name: "ohos.permission.INTERNET",
        reason: "$string:reason_internet",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "inuse",
        },
      },

      {
        name: "ohos.permission.RECEIVER_STARTUP_COMPLETED",
        reason: "$string:reason_internet",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "always",
        },
      },

      {
        name: "ohos.permission.CONNECTIVITY_INTERNAL",
        reason: "$string:reason_internet",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "always",
        },
      },

      {
        name: "ohos.permission.GET_RUNNING_INFO",
        reason: "$string:reason_running_info",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "inuse",
        },
      },

      {
        "name": "ohos.permission.GET_WIFI_INFO",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        },
      },
      {
        "name": "ohos.permission.SET_WIFI_INFO",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.LOCATION",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.APPROXIMATELY_LOCATION",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }},
      {
        "name": "ohos.permission.GET_WIFI_INFO_INTERNAL",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_WIFI_PEERS_MAC",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_WIFI_LOCAL_MAC",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_WIFI_CONFIG",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.SET_WIFI_CONFIG",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.MANAGE_WIFI_CONNECTION",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.MANAGE_WIFI_HOTSPOT",
        "reason": "$string:reason_internet",
        "usedScene": {
          "abilities": [
            "MainAbility"
          ],
          "when": "inuse"
        }
      },
      {
        name: "ohos.permission.GET_NETWORK_INFO",
        reason: "$string:reason_network_info",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "inuse",
        },
      },
      {
        name: "ohos.permission.MANAGE_APP_BOOT",
        reason: "$string:reason_boot_start",
        usedScene: {
          abilities: ["EntryAbility"],
          when: "always",
        },
      },
    ],
    abilities: [
      {
        name: "EntryAbility",
        srcEntry: "./ets/entryability/EntryAbility.ets",
        description: "$string:EntryAbility_desc",
        icon: "$media:layered_image",
        label: "$string:EntryAbility_label",
        startWindowIcon: "$media:startIcon",
        startWindowBackground: "$color:start_window_background",
        exported: true,
        skills: [
          {
            entities: ["entity.system.home"],
            actions: ["action.system.home"],
          },
        ],
      },
    ],
    extensionAbilities: [
      {
        name: "EntryBackupAbility",
        srcEntry: "./ets/entrybackupability/EntryBackupAbility.ets",
        type: "backup",
        exported: false,
        metadata: [
          {
            name: "ohos.extension.backup",
            resource: "$profile:backup_config",
          },
        ],
      },
    ],
  },
}
